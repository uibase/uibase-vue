{
  "name": "@uibase/uibase-vue",
  "version": "0.7.3-beta0.1",
  "description": "base ui by vue.",
  "main": "src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "script:dev": "npx tsc -w --outDir ./bin ./src/scripts/main.ts",
    "script:build": "npx tsc --build ./tsconfig.json",
    "dev": "webpack-dev-server --config build/webpack.test.config.js --hot --progress -d",
    "build": "npm run script:build && npm run theme:build && cross-env NODE_ENV=production webpack --config build/webpack.config.js --progress && npm run ccss:build",
    "ccss": "node-sass --importer src/assets/css/importer.js src/assets/css/base.scss src/assets/css/base.css",
    "ccss:build": "node-sass --importer src/assets/css/importer.js src/assets/css/base.scss dist/base.ui.common.css",
    "theme:build": "node ./bin/scripts/main.js create",
    "styleguide": "vue-styleguidist server --config ./build/styleguide.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:uibase/uibase-vue.git"
  },
  "bin": {
    "uibase": "./bin/scripts/main.js"
  },
  "keywords": [
    "UI"
  ],
  "author": "junzo matsunoo <no.diggi.t@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "chokidar": "^3.4.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.4.1",
    "mini-css-extract-plugin": "^0.11.1",
    "node-sass": "^4.14.0",
